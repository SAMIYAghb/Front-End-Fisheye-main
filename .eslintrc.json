{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/extensions": ["error", "always", {
            "js": "never'"
          }],
    //    "operator-linebreak": ["error", "before"],
        "eqeqeq": ["error", "always"],
        // "no-param-reassign": ["error", { "props": true }],
        "no-param-reassign": "off",
        "no-lonely-if": "off"
            // "max-classes-per-file": ["error", 3]
        


    }
}
